<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">

            <corners android:radius="20dp" />
            <gradient
                android:centerColor="#0055C4FF"
                android:endColor="#0055C4FF"
                android:startColor="#55C4FF" />
        </shape>
    </item>
    <item
        android:bottom="1dp"
        android:left="1dp"
        android:top="1dp">
        <shape android:shape="rectangle">

            <corners android:radius="20dp" />
            <gradient
                android:centerColor="#8017ABD3"
                android:endColor="#002DC5D3"
                android:startColor="#0090D3" />
        </shape>
    </item>


</layer-list>

