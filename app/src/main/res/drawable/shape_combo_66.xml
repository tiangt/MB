<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">

            <corners android:radius="20dp" />
            <gradient
                android:centerColor="#00B9FFC0"
                android:endColor="#00B9FFC0"
                android:startColor="#B9FFC0" />
        </shape>
    </item>
    <item
        android:bottom="1dp"
        android:left="1dp"
        android:top="1dp">
        <shape android:shape="rectangle">

            <corners android:radius="20dp" />
            <gradient
                android:centerColor="#993FDB6A"
                android:endColor="#003FDB6A"
                android:startColor="#3FDB6A" />
        </shape>
    </item>


</layer-list>

